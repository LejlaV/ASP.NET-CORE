@using RS1_Ispit_asp.net_core.ViewModels
@model RezultatiTakmicenjaVM
@{
    ViewData["Title"] = "Rezultati";
}

<h2>Rezultati takmicenja</h2>

<form class="form-group">

    <input asp-for="@Model.TakmicenjeID" hidden />

    <div class="form-group">
        <label>Skola</label>
        <input asp-for="@Model.Skola" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label>Predmet</label>
        <input asp-for="@Model.Predmet" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label>Razred</label>
        <input asp-for="@Model.Razred" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label>Datum</label>
        <input asp-for="@Model.Datum" class="form-control" readonly />
    </div>
</form>

<a asp-controller="Takmicenje" asp-action="Zakljucaj" asp-route-TakmicenjeID="@Model.TakmicenjeID"
   class="btn btn-success">Zakljucaj</a>

<div id="ajaxDiv"></div>
<script>
	$.get("/Takmicenje/TakmicenjeUcesnikIndex?TakmicenjeID=@Model.TakmicenjeID",
		function (result, status) {
			$("#ajaxDiv").html(result);
		});
</script>
